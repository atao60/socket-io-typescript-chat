  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -n \"proxy,server\" -c \"green.bold,yellow.bold\" \"npm run client:start\" \"npm run server:start\" ",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:start": "node --inspect=5858 -r ts-node/register ./server/src/index.ts",
    "client:start": "ng serve --open"
  },
  "private": true,
